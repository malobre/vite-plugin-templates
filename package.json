    "check": "tsc",
    "prepare": "tsup src/index.ts --format=esm --dts --clean --sourcemap"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.12.11",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "magic-string": "^0.30.10",
    "parse5": "^7.1.2"
  }
}
