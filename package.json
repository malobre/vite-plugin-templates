    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "magic-string": "^0.30.17",
    "parse5": "^7.2.1"
  }
